{
  "name": "Simple History Cleaner",
  "version": "1.0.2",
  "manifest_version": 3,
  "description": "起動時に一定期間経過した履歴・キャッシュを削除",
  "background": {
    "service_worker": "/js/background.js"
  },
  "icons": {
    "128": "/img/icon128.png"
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": false
  },
  "permissions": ["browsingData", "history", "downloads", "storage"]
}